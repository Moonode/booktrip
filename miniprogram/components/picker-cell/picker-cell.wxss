    margin-left: 12rpx;
}

.text {
    width: 520rpx;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
}

.down {
    width: 568rpx;
    margin-top: 20rpx;
    border: 1rpx solid rgba(112, 112, 112, 1);
    opacity: 0.1;
}